<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<h1>
			<%= @wiki.title %>
			<small>
				Originally created by <%= @wiki.user_id %>
			</small>
			<span class="pull-right"><%= link_to "Delete Wiki", @wiki, method: :delete, class: 'btn btn-danger' %></span>
			<span class="pull-right marg"><%= link_to "Edit Wiki", edit_wiki_path(@wiki), class: 'btn btn-success' %></span>
		</h1>
		<div>
			<%= @wiki.body %>
		</div>
	</div>
</div>